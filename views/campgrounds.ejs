<% include partials/header %>
<header class="jumbotron">
    <div class="container">
        <h1>Campgrounds</h1>
        <p>
        <a class="btn btn-primary btn-lrg" href="/campgrounds/new">Add New Campground</a>
        </p>
    </div>
</header>
<div class="container">
    <div class="row text-center">
        <% campgrounds.forEach((campgrounds)=> { %>
        <div class="col-lg-3 col-md-4 col-sm-6">
            <div class="img-thumbnail">
                <img src="<%= campgrounds.image %>" />
                <div class="caption">
                    <h4><%= campgrounds.name %></h4>
                </div>
            </div>
        </div>
        <% }); %>
    </div>
</div>


<% include partials/footer %>